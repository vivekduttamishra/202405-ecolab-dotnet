@model ConceptArchitect.BookManagement.Author

@section title{
    @Model.Name
}

<h2>@Model.Name</h2>

<div class="author_details">
    <div class="photo">
        <img src="@Model.Photograph"/>
        <a href="/authors/delete/@Model.Id">Delete</a>
    </div>
    <div class="details">
        <h3>Biography</h3>
        <p>@Model.Biography</p>
    </div>
</div>
